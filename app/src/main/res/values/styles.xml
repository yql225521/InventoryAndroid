<?xml version="1.0" encoding="UTF-8"?>
<resources xmlns:android="http://schemas.android.com/apk/res/android">
	<style name="roomRatingBar" parent="@android:style/Widget.RatingBar">
		<item name="android:progressDrawable">@drawable/room_rating_bar</item>
		<item name="android:minHeight">32dp</item>
		<item name="android:maxHeight">32dp</item>
	</style> 
	<style name="roomRatingBarMini" parent="@android:style/Widget.RatingBar">
		<item name="android:progressDrawable">@drawable/room_rating_bar_min</item>
		<item name="android:minHeight">16dp</item>
		<item name="android:maxHeight">16dp</item> 
	</style> 
	<style name="fs_verify_info_dic_title_text">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">14sp</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:paddingLeft">10dp</item>
		<item name="android:paddingRight">10dp</item>
	</style>
	<style name="fs_verify_info_dic_title_division">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:src">@drawable/fs_verify_info_division</item>
	</style>
	<style name="fs_verify_info_verValueText">
		<item name="android:layout_marginRight">2dp</item>
		<item name="android:layout_marginTop">5dp</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:saveEnabled">true</item>
		<item name="android:inputType">text</item>
	</style>
	<style name="fs_verify_info_verText">
		<item name="android:textSize">18sp</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:paddingLeft">2dp</item>
		<item name="android:layout_marginRight">2dp</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginTop">13dp</item>
	</style>
	<style name="oa_pend_work_info_btn_more">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:background">@drawable/oa_pend_work_info_more_selector</item>
		<item name="android:layout_marginBottom">3dp</item>
		<item name="android:layout_marginRight">3dp</item>
	</style>
	<style name="oa_pend_work_info_edit">
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">#ff3f3f3f</item>
		<item name="android:singleLine">true</item>
	</style>
	<style name="oa_pend_work_info_text">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">#ff3f3f3f</item>
		<item name="android:layout_marginRight">3dp</item>
		<item name="android:layout_marginTop">8dp</item>
		<item name="android:layout_alignParentLeft">true</item>
	</style>
	<style name="list_search_no_data_text">
		<item name="android:textSize">18sp</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_centerInParent">true</item>
		<item name="android:visibility">gone</item>
		<item name="android:layout_gravity">center</item>
	</style>
	<style name="adm_doc_creat_document_title_text">
		<item name="android:textSize">18sp</item>
		<item name="android:textColor">@color/white</item>
		<item name="android:paddingLeft">2dp</item>
		<item name="android:layout_marginRight">2dp</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:layout_marginTop">13dp</item>
	</style>
	<style name="adm_doc_creat_document_value_text">
		<item name="android:layout_marginRight">2dp</item>
		<item name="android:layout_marginTop">5dp</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
	</style>
	<style name="detail_title_text">
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:paddingLeft">5dp</item>
		<item name="android:background">#cccccc</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:layout_width">fill_parent</item>
		<item name="android:layout_height">wrap_content</item>
	</style>
	<style name="detail_value_text">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@color/black</item>
		<item name="android:layout_marginLeft">35dp</item>
		<item name="android:gravity">center_vertical</item>
		<item name="android:layout_marginTop">5dp</item>
		<item name="android:layout_marginBottom">5dp</item>
		<item name="android:paddingRight">5dp</item>
	</style>
    <style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">@color/qq_checkbox_text_color</item>
        <item name="android:paddingLeft">28.0px</item>
        <item name="android:button">@drawable/qq_btn_check</item>
    </style>
    <style name="MyDialogStyle">
        <item name="android:textAppearance">@android:style/TextAppearance.DialogWindowTitle</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowNoTitle">true</item>
        <!--<item name="android:maxLines">1</item>
        <item name="android:scrollHorizontally">true</item>-->
    </style>
    <style name="QQAlertDialog" parent="@android:style/Theme.Wallpaper" />
    <style name="MyListView" parent="@android:style/Widget.ListView">
        <item name="android:listSelector">@drawable/selectitem</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:divider">@drawable/line</item>
    </style>
    <style name="QQButton" parent="@android:style/Widget.Button">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:background">@drawable/button</item>
    </style>
    <style name="QQEditText" parent="@android:style/Widget.EditText">
        <item name="android:textSize">16.0sp</item>
        <item name="android:textColorHighlight">#ffe1e1e1</item>
        <item name="android:textColorHint">#ffcbd4d9</item>
        <item name="android:background">@drawable/qq_edit_text1</item>
    </style>
 
    <style name="QQRadioButton" parent="@android:style/Widget.CompoundButton.RadioButton">
        <item name="android:textColor">@color/qq_checkbox_text_color</item>
        <item name="android:button">@drawable/qq_btn_radio</item>
    </style>
    <style name="QQProgressBar" parent="@android:style/Widget.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progressbar</item>
    </style>
    <style name="MyGridView" parent="@android:style/Widget.GridView">
        <item name="android:listSelector">@null</item>
        <item name="android:cacheColorHint">#00000000</item>
        <item name="android:divider">@drawable/line</item>
    </style>
    
    <!-- 设置title的颜色 -->
    <style name="CustomWindowTitleBackground">
        <item name="android:background">#3a74aa</item>
        <item name="android:paddingLeft">5dp</item>
    </style>
    
    <style name="ThemeDefault" parent="@android:style/Theme"> 
	    <item name="android:buttonStyle">@style/QQButton</item>
	    <item name="android:windowContentOverlay">@null</item>
	    <item name="android:alertDialogStyle">@style/QQAlertDialog</item>
	    <item name="android:scrollbarThumbVertical">@drawable/qq_scrollbar</item>
	    <item name="android:editTextStyle">@style/QQEditText</item>
	    <item name="android:listViewStyle">@style/MyListView</item> 
	    <item name="android:progressBarStyle">@style/QQProgressBar</item>
	    <item name="android:radioButtonStyle">@style/QQRadioButton</item>
	    <item name="android:gridViewStyle">@style/MyGridView</item>
	    <item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item>
    </style>
</resources>